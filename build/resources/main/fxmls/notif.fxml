<AnchorPane prefHeight="95.0" prefWidth="784.0" style="-fx-background-color: yellow;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ButtonBar layoutX="495.0" layoutY="15.0" prefHeight="42.0" prefWidth="244.0">
        <buttons>
            <Button fx:id="acceptBtn" mnemonicParsing="false" onAction="#accept" prefHeight="26.0" prefWidth="131.0" style="-fx-background-color: green;" text="Accept" />
          <Button fx:id="rejectBtn" minWidth="0.0" mnemonicParsing="false" onAction="#reject" prefHeight="26.0" style="-fx-background-color: red;" text="Reject" />
            <Button fx:id="removeBtn" mnemonicParsing="false" onAction="#remove" style="-fx-background-color: gray;" text="Remove" />
        </buttons>
      </ButtonBar>
      <Label fx:id="textLabel" layoutX="86.0" layoutY="5.0" prefHeight="66.0" prefWidth="424.0" text="Label" />
      <Circle fx:id="avatar" fill="DODGERBLUE" layoutX="40.0" layoutY="41.0" radius="26.0" stroke="BLACK" strokeType="INSIDE" />
      <ToolBar layoutY="84.0" prefHeight="17.0" prefWidth="784.0" style="-fx-background-color: black;" />
   </children>
</AnchorPane>
